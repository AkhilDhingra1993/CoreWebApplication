@model IEnumerable<CoreWebApplication.Models.User>
<html>
<head>
    <title></title>
</head>
<body>
    <table>
        <thead>
            <tr>
                <th>ID</th>
                <th>Email</th>
                <th>First Name</th>
                <th>Last Name</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var user in Model)
            {
                <tr>
                    <td>@user.ID</td>
                    <td>@user.EMAIL</td>
                    <td>@user.FIRST_NAME</td>
                    <td>@user.LAST_NAME</td>
                </tr>
            }
        </tbody>
    </table>
</body>
</html>
